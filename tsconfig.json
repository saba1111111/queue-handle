{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "libs/queue": [
        "libs/queue/src"
      ],
      "libs/queue/*": [
        "libs/queue/src/*"
      ],
      "libs/auth": [
        "libs/auth/src"
      ],
      "libs/auth/*": [
        "libs/auth/src/*"
      ],
      "libs/mail": [
        "libs/mail/src"
      ],
      "libs/mail/*": [
        "libs/mail/src/*"
      ]
    }
  }
}